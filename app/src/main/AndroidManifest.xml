<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.thealer.telehealer">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.USE_BIOMETRIC" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE" />
    <uses-permission android:name="android.permission.SET_ALARM" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name=".TeleHealerApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".views.common.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".views.home.HomeActivity" />
        <activity android:name=".views.onboarding.OnBoardingActivity" />
        <activity android:name=".views.signup.SignUpActivity" />
        <activity android:name=".views.signup.PasswordRequirementActivity" />
        <activity android:name=".views.quickLogin.QuickLoginActivity" />
        <activity android:name=".views.signin.SigninActivity" />
        <activity android:name=".views.signin.ForgotPassword" />
        <activity android:name=".views.common.QRCodeReaderActivity" />
        <activity android:name=".views.proposer.ProposerActivity" />
        <activity android:name=".views.home.AddConnectionActivity" />
        <activity android:name=".views.settings.ProfileSettingsActivity" />
        <activity android:name=".views.settings.DeleteAccountActivity" />
        <activity android:name=".views.home.orders.CreateOrderActivity" />
        <activity android:name=".views.home.DoctorOnBoardingActivity" />
        <activity android:name="iHealth.pairing.VitalCreationActivity" />
        <activity android:name=".views.home.vitals.BluetoothEnableActivity" />
        <activity android:name=".views.home.schedules.CreateNewScheduleActivity" />
        <activity android:name=".views.inviteUser.InviteUserActivity" />
        <activity android:name=".views.common.ContentActivity" />
        <activity android:name=".views.call.CallFeedBackActivity" />
        <activity android:name=".views.home.EmailVerificationActivity" />
        <activity
            android:name=".views.common.AppUpdateActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".views.call.CallActivity"
            android:launchMode="singleTop" />

        <activity
            android:name=".views.settings.SignatureActivity"
            android:screenOrientation="userLandscape" />
        <activity
            android:name=".views.common.CallPlacingActivity"
            android:theme="@style/AppTheme.Transparent" />
        <activity
            android:name="com.braintreepayments.api.BraintreeBrowserSwitchActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="${applicationId}.braintree" />
            </intent-filter>
        </activity>


        <activity
            android:name=".views.notification.NotificationActivity"
            android:parentActivityName=".views.home.HomeActivity" />

        <activity android:name=".views.notification.NotificationDetailActivity" />
        <activity android:name=".views.call.CallNetworkTestActivity" />

        <receiver
            android:name=".common.BTStateChangedBroadcastReceiver"
            android:enabled="true">
            <intent-filter>
                <action android:name="android.bluetooth.adapter.action.STATE_CHANGED" />
            </intent-filter>
        </receiver>

        <service android:name=".common.pubNub.TelehealerFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service
            android:name=".common.OpenTok.CallMinimizeService"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <receiver
            android:name=".views.home.schedules.LocalNotificationReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.media.action.DISPLAY_NOTIFICATION" />
                <action android:name="android.intent.action.NOTIFY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>

        <receiver android:name=".views.notification.NotificationCancelAppointmentReceiver" />

        <activity android:name=".views.inviteUser.InviteContactUserActivity" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="${fabric_key}" />

        <activity android:name=".views.home.pendingInvites.PendingInvitesActivity" />

        <activity android:name=".views.quickLogin.QuickLoginAgreementActivity" />
    </application>

</manifest>